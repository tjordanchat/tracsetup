gpg --gen-key # Sasha
gpg --gen-key # Larry
gpg --list-keys
gpg -ao Sashas_pub --export Sasha
gpg -ao Larrys_pub --export Larry
gpg -ao Larrys_secret --export-secret-key Larry
gpg -ao Sashas_secret --export-secret-key Sasha
vi tt.c
gpg -e -r Larry tt.c
od -c tt.c.gpg
cp tt.c.gpg tmp
cd tmp
gpg tt.c.gpg
ls -l tt.c
